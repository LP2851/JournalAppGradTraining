<h1>All Entries</h1>
<table>
  <tr>
    <th>Title</th>
    <th>URL</th>
    <th></th>
  </tr>
  <% @entries.each do |entry| %>
    <tr>
      <td>
        <%=entry.title %>
      </td>
      <td>
        <%=entry.url %>
      </td>
      <td>
        <%= button_to "Delete", entry, method: :delete, data: { confirm: "Are you sure you want to delete this Project ?"} %>
      </td>
    </tr>
  <% end %>
</table>